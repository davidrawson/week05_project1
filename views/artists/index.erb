<div class="add_form">
  <thead>
    <tr>
      <th>
        Artists
      </th>
      <th>
        <caption></caption>
      </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        <% @artists.each do |artist| %>

        <h3><%= artist.name() %></h3>
      </td>
      <td>
        <form action="/artists/<%= artist.id %>/edit" method="GET">
          <input class="btn" type="submit" value="Update Artist" />
        </form>
      </td>
    </tr>

    <% end %>

  </tbody>
</div>
