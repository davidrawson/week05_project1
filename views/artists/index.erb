<div class="Artist_table">
<thead>
  <th>
    <h1>Artists</h1>
  </th>
</thead>
<tbody>
  <tr>
    <td>
      <% @artists.each do |artist| %>

      <h3><%= artist.name() %></h3>
      <form action="/artists/<%= artist.id %>/edit" method="GET">
        <input class="btn" type="submit" value="Update Artist" />
      </form>
    </td>
  </tr>

<% end %>

</tbody>
</div>
